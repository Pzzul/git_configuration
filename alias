#Alias for git Command
command line: git config --global alias.[alias] [true command]

alias      true command
 co      =  checkout
 br      =  branch
 ci      =  commit
 st      =  status
 unstage =  reset HEAD
 last    =  log -1 HEAD
 visual  =  gitk
 colog	 =  log --oneline --graph --decorate --all
